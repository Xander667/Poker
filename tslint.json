{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "ordered-imports": "any",
    "named-imports-order": "any",
    "no-console": false,
    "no-namespace": false,
    "curly": [true, "ignore-same-line"]
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
